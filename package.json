{
  "name": "yuval-cohen-portfolio",
  "version": "0.1.0",
  "private": true,
  "bundleDependencies": true,
  "scripts": {
    "nextjs": "node --allow-worker /node_modules/.bin/next",
    "start": "NEXT_MANUAL_SIG_HANDLE=true next start",
    "serve": "npx run serve dist",
    "dev": "next dev --experimental-https",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "postbuild": "",
    "prepare": "node .husky/install.mjs",
    "spell": "npx cspell .",
    "lint-staged": "lint-staged",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format:check": "prettier --log-level=log --check .",
    "format": "prettier --write .",
    "test:ci": "jest --ci --runInBand --coverage",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean:all": "rm -rf node_modules dist package-lock.json public/fallback* public/sw.js public/workbox* coverage .swc .next cache .husky/_"
  },
  "dependencies": {
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "lodash": "4.17.21",
    "image-size": "1.2.0",
    "sharp": "0.33.5",
    "next": "15.1.6",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "5.3.0",
    "nprogress": "0.2.0",
    "framer-motion": "11.5.6",
    "tailwind-merge": "2.5.3",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "1.0.7",
    "@tawk.to/tawk-messenger-react": "2.0.2",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/auto-instrumentations-web": "^0.45.0",
    "@opentelemetry/context-zone": "^1.30.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.57.0",
    "@opentelemetry/instrumentation": "^0.57.0",
    "@opentelemetry/instrumentation-fetch": "0.57.0",
    "@opentelemetry/resources": "^1.30.1",
    "@opentelemetry/sdk-trace-web": "^1.30.0",
    "@opentelemetry/semantic-conventions": "^1.28.0",
    "next-pwa": "5.6.0"
  },
  "devDependencies": {
    "typescript": "5.7.3",
    "@next/bundle-analyzer": "14.2.14",
    "@next/eslint-plugin-next": "14.2.14",
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/nprogress": "0.2.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "@testing-library/react": "16.2.0",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "eslint": "9.18.0",
    "eslint-config-next": "15.1.4",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-tailwindcss": "3.17.5",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.30",
    "husky": "9.1.7",
    "lint-staged": "15.4.3",
    "cspell": "8.17.3",
    "ts-node": "10.9.2",
    "webpack-obfuscator": "3.5.1",
    "ua-parser-js": "2.0.0",
    "run": "1.5.0",
    "serve": "14.2.4"
  },
  "prettier": {
    "jsxSingleQuote": false,
    "trailingComma": "none",
    "singleAttributePerLine": true,
    "semi": true
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:fix"
    ],
    "**/*.{json,md,yml}": [
      "npm run format"
    ],
    "**/*.{js,jsx,ts,tsx}": [
      "npm run lint:fix",
      "npm run format"
    ],
    "**/*.{css,scss,md,json,yml}": [
      "npm run format"
    ]
  }
}
